<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Color.DarkBlue">#FF0A3655</Color>
    <Color x:Key="Color.White">#FFFFFFFF</Color>

    <SolidColorBrush x:Key="Brush.DarkBlue" Color="{StaticResource Color.DarkBlue}"/>
    <SolidColorBrush x:Key ="Brush.White" Color ="{StaticResource Color.White}"/>
</ResourceDictionary>
